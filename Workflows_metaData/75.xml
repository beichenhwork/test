ag>    <tag uri="http://www.myexperiment.org/tag.xml?id=119" resource="http://www.myexperiment.org/tags/119" id="119">disease</tag>    <tag uri="http://www.myexperiment.org/tag.xml?id=38" resource="http://www.myexperiment.org/tags/38" id="38">BioAID</tag>    <tag uri="http://www.myexperiment.org/tag.xml?id=5" resource="http://www.myexperiment.org/tags/5" id="5">AIDA</tag>    <tag uri="http://www.myexperiment.org/tag.xml?id=522" resource="http://www.myexperiment.org/tags/522" id="522">VL-e</tag>    <tag uri="http://www.myexperiment.org/tag.xml?id=611" resource="http://www.myexperiment.org/tags/611" id="611">biorange_nl</tag>    <tag uri="http://www.myexperiment.org/tag.xml?id=612" resource="http://www.myexperiment.org/tags/612" id="612">text_mining_network</tag>  </tags></workflow>