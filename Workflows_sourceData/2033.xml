ors>                        <executableAuthors>                                <author fullName="Farhad Hormozdiari" email="" website="http://eichlerlab.gs.washington.edu/" />                                <author fullName="Faraz Hach" email="" website="http://eichlerlab.gs.washington.edu/" />                        </executableAuthors>                        <citations>                                <citation>PMID: 2875196</citation>                                <citation>PMID: 20676076</citation>                        </citations>                        <license>LGPL</license>                        <tag>informatics</tag>                        <tag>fasta</tag>                        <tag>sequence</tag>                        <tag>alignment</tag>                        <uri>http://mrfast.sourceforge.net/manual.html</uri>                        <metadata>                                <data key="__creationDateKey" value="Mon Aug 30 12:20:56 PDT 2010" />                        </metadata>                        <input id="FASTAIndexing_1.FastaInputFile" name="Fasta Input File" description="Upon the completion of the indexing phase, you can find &quot;refgen.fasta.index&quot; in the same directory as &quot;refgen.fasta&quot;. mrFAST uses a window size of 12 (default) to make the index of the genome, this windows size can be modified with &quot;--ws&quot;. There is a restriction on the maximum of the window size as the window size directly affects the memory usage. " required="true" enabled="true" order="0" switch="--index" switchSpaced="true">                                <format type="File" cardinality="1">                                        <fileTypes>                                                <filetype name="Fasta" extension="fasta" description="BLAST input query FASTA file" />                                        </fileTypes>                                </format>                                <dependencies>                                  