<wf:SequentialWorkflowActivity  xmlns:ps="http://pha.jhu.edu/PanSTARRS/2008/Workflow/Load/Activity"  xmlns:psc="http://pha.jhu.edu/PanSTARRS/2008/Workflow/Common/Activity"  xmlns:wf="http://schemas.microsoft.com/winfx/2006/xaml/workflow"  xmlns:x="http://schemas.microsoft.com/winfx/xaml">  <ps:GetCSVBatchEntryActivity x:Name="GetCSVBatchEntry"/>  <ps:TestCSVReadyFileExistsActivity x:Name="TestCSVReadyFileExists" ps:InputCSVBatchStatus="{wf:ActivityBind GetCSVBatchEntry,Path=OutputCSVBatchStatus}"/>  <wf:IfElseActivity x:Name="IfNotCSVReadyFileExistsActivity">    <wf:IfElseBranchActivity x:Name="IfNotCSVReadyFileExists">      <wf:IfElseBranchActivity.Condition>        <psc:ConditionPassthruActivity psc:InputCondition="{wf:ActivityBind TestCSVReadyFileExists,Path=OutputTestSuccess}" />      </wf:IfElseBranchActivity.Condition>      <!-- insert fault activity -->      <wf:TerminateActivity x:Name="TerminateOnNotCSVReadyFileExists" />    </wf:IfElseBranchActivity>  </wf:IfElseActivity>  <ps:InitCSVBatchEntryActivity x:Name="InitCSVBatchEntry" ps:InputCSVBatchStatus="{wf:ActivityBind TestCSVReadyFileExists,Path=OutputCSVBatchStatus}" />    <ps:TestCSVFileCountActivity x:Name="TestCSVFileCount" ps:InputCSVBatchStatus="{wf:ActivityBind InitCSVBatchEntry,Path=OutputCSVBatchStatus}" />  <wf:ReplicatorActivity x:Name="ForEachCSVFileEntryDo1" x:ExecutionType="Sequence"    x:InitialChildData="{wf:ActivityBind TestCSVFileCount, Path=OutputCSVBatchStatus.Entry.CSVFileStatusListNonGeneric}"> <!-- tempdel CSVFileStatusList-->    <wf:SequenceActivity x:Name="TestCSVFileExistsContainer">      <ps:ReplicatorCurrentCSVFileStatusActivity x:Name="ForEachCSVFileEntryDo1Item" />      <ps:TestCSVFileExistsActivity x:Name="TestCSVFileExists" ps:InputCSVFileStatus="{wf:ActivityBind ForEachCSVFileEntryDo1Item,Path=CurrentItem}" ps:InputRootUNCPath="{wf:ActivityBind TestCSVFileCount, Path=OutputCSVBatchStatus.Entry.RootUNCPath}" />      <ps:TestCSVFileChecksumActivity x:Name="TestCSVFileChecksum" ps:InputCSVFileStatus="{wf:ActivityBind TestCSVFileExists,Path=OutputCSVFileStatus}" ps:InputRootUNCPath="{wf:ActivityBind TestCSVFileCount, Path=OutputCSVBatchStatus.Entry.RootUNCPath}" />      <ps:InitCSVFileEntryActivity x:Name="InitCSVFileEntry" ps:InputCSVFileStatus="{wf:ActivityBind TestCSVFileChecksum,Path=OutputCSVFileStatus}" ps:InputRootUNCPath="{wf:ActivityBind TestCSVFileCount, Path=OutputCSVBatchStatus.Entry.RootUNCPath}" />      <!-- TODO: merge all updated CSV File Status outputs back into a List -->    </wf:SequenceActivity>          </wf:ReplicatorActivity>  <ps:InitCSVBatchLoadDBActivity x:Name="InitCSVBatchLoadDB" ps:InputCSVBatchStatus="{wf:ActivityBind TestCSVFileCount, Path=OutputCSVBatchStatus}" ps:InputCSMConnectionString="DEFAULT" /> <!-- TODO: Implement this -->  <!-- TODO: Use merged resultof updated CSV File Status outputs from replicator1 -->  <wf:ReplicatorActivity x:Name="ForEachCSVFileEntryDo2" x:ExecutionType="Sequence"    x:InitialChildData="{wf:ActivityBind InitCSVBatchLoadDB, Path=OutputCSVBatchStatus.Entry.CSVFileStatusListNonGeneric}"> <!-- tempdel CSVFileStatusList-->    <wf:SequenceActivity x:Name="LoadAndTestCSVFileContainer">      <ps:ReplicatorCurrentCSVFileStatusActivity x:Name="ForEachCSVFileEntryDo2Item" />            <ps:TestCSVFileColumnsActivity x:Name="TestCSVFileColumns" ps:InputCSVFileStatus="{wf:ActivityBind ForEachCSVFileEntryDo2Item,Path=CurrentItem}" ps:InputLoadDBGuid="{wf:ActivityBind InitCSVBatchLoadDB,Path=OutputLoadDBGuid}" ps:InputCSMConnectionString="DEFAULT" /> <!-- TODO: Fix Bugs in Impl -->            <!--<ps:LoadCSVFileTableActivity /> --> <!-- TODO: Implement this -->            <!--<ps:UpdateCSVFileTableHTMActivity />--><!-- TODO: Wrap these -->      <!--<ps:TestCSVFileTableUniquePKActivity />      <ps:TestCSVFileCountActivity />-->          </wf:SequenceActivity>  </wf:ReplicatorActivity>  </wf:SequentialWorkflowActivity>