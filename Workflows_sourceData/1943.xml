PREFIX sioc: <http://rdfs.org/sioc/ns#>PREFIX dcterms: <http://purl.org/dc/terms/>PREFIX mebase: <http://rdf.myexperiment.org/ontologies/base/>PREFIX mecontrib: <http://rdf.myexperiment.org/ontologies/contributions/>PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>SELECT ?name (COUNT(?workflow) as ?no_workflows)WHERE{  ?workflow rdf:type mecontrib:Workflow ;    sioc:has_owner ?owner ;    mebase:has-content-type ?ct .  ?owner sioc:name ?name .  ?ct dcterms:title ?ct_title ;  FILTER regex(?ct_title,'^taverna','i')}GROUP by ?ownerORDER BY ?no_workflows